{
  "manifestVersion": 1,
  "extensionId": "private-deploy-ssrs",
  "name": "Deploy SSRS - Retry",
  "version": "1.0.10",
  "publisher": "VictorPaulNoel",
  "description": "Deploy SSRS packages",
  "public": false,
  "galleryFlags": [
    "Private"
  ],
  "icons": {
    "default": "images/logo.png",
    "large": "images/logo-large.png"
  },
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "SQL Server Reporting Services",
    "Reporting",
    "Reports",
    "SSRS",
    "SQL",
    "rdl"
  ],
  "links": {
    "home": {
      "uri": "http://blog.majcica.com/"
    },
    "support": {
      "uri": "https://github.com/mmajcica/DeploySsrs/issues"
    },
    "repository": {
        "uri": "https://github.com/mmajcica/DeploySsrs"
    },
    "issues": {
        "uri": "https://github.com/mmajcica/DeploySsrs/issues"
    }
  },
  "repository": {
      "type": "git",
      "uri": "https://github.com/devpauln/DeploySsrs"
  },
  "branding": {
    "color": "#e9ebed",
    "theme": "light"
  },
  "screenshots": [
    {
        "path": "images/task-ssrs.png"
    }
  ],
  "content": {
    "details": {
        "path": "README.md"
    },
    "license": {
        "path": "LICENSE.txt"
    }
  },
  "files": [
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "task"
    }
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "DeploySsrsRetry",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "task"
      }
    }
  ]
}
